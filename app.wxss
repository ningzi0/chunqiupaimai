/**app.wxss**/
.container {
  height: 100%;
  padding: 0 2%;
  box-sizing: border-box;
  font-weight: 200;
} 
::-webkit-scrollbar {
 
  width: 0;
   
  height: 0;
   
  color: transparent;
}
/*清楚浮动*/
.clearfix:after {
  clear: both;
  height: 0;
  content: "";
  line-height: 0;
  font-size: 0;
  display: block;
}



.tab-content{
  min-height: 15em;
}
.tab-content .ul .li{
  width: 49%;
  display: inline-block;
  margin:0 2% 2% 0;
}
.tab-content .ul .li:nth-child(2n){
  margin-right: 0;
}
.tab-content .ul .li image{
  width: 100%;
  height: 8em;
  display: block;
}
.t_ctit{
  font-size: 0.85em;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding: 0.35em;
  background: #ededed;
}
.tabinfo{
  padding: 0.35em;
}
.tabCon{
  background: #fff;
}
.tabinfoItemTit{
  font-size: 0.85em;
  display: inline-block;
  width: 3em;
}
.tabinfoItem{
  margin: 0.2em 0;
  color: #a1a7ab;
  letter-spacing: -0.1em;
}
.tabinfoPrice{
  font-size: 1.5em;
  font-weight: bold;
  margin-left: 0.35em;
  color: #d90f09;
}
.tabinfo_price{
  margin-left: 0.35em;
  font-size: 0.85em;
}
.tabinfoBtn{
  font-size: 0.85em;
  background: #d90f09;
  border-radius: 3em 0 0 3em;
  color: #fff;
  width:50%;
  text-align: center;
  float: right;
}
.tabinfoBottom{
  display: flex;
  margin-top: 0.5em;
  border-top: 1px solid #eaebef;
}
.tabinfoBottom .tabinfoB_item{
  flex: 1;
  font-size:0.8em;
  padding: 1em 0.5em;
  color: #b3b5b6;
  border-right: 1px solid #eaebef;
}
.tabinfoBottom .tabinfoB_item:last-child{
  border-right: 0;
}
.tabinfoBottom .tabinfoB_item .b{
  color: #555c5e;
}



/* table切换样式 */
